snippet controller "Laravel - Basic Controller"
	
	<?php
	
	class ${1:${TM_FILENAME/(.+)\..+|.*/\u$1_Controller/:ControllerName}} extends ${2:Base_}Controller
	{
	
		$0
	
	}
	
	
endsnippet

snippet elif
	
	@if(${1:condition})
	    ${3:execute}
	@elseif(${2:else_contidtion})
	    ${4:execute if else}
	@endif
	
endsnippet

snippet else
	
	@if(${1:condition})
	    ${2:execute}
	@else
	    ${3:execute if else}
	@endif
	
endsnippet

snippet fore
	
	@foreach(${1:$vars} as ${2:$var})
	
	    {{ ${3:$var} }}
	    
	@endforeach
	
endsnippet

snippet forel
	
	@forelse (${1:$vars} as ${2:$var})
	    {{ ${3:$var} }}
	@empty
	    ${4:// message}
	@endforelse
	
endsnippet

snippet if
	
	@if(${1:condition})
	    ${2:execute}
	@endif
	
endsnippet

snippet inc
	
	@include('${1:filename}')
	
endsnippet

snippet lay
	
	@layout('${1:template}')
	
	@section('${2:content}')
	
	    ${3:// more code}
	    
	@endsection
	
endsnippet

